{
  "name": "gxgd-check",
  "version": "1.2.0",
  "author": "chenxin.azurewind <chenxin.az@outlook.com>",
  "description": "GXGD 数据核查工具",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build:cmd": "node ./build/build.js && electron-packager . --ignore=/\\. --ignore=/yarn --ignore=/test --out=dist/exe --icon=./static/logo.ico --asar --overwrite --arch=ia32,x64 --platform=win32",
    "build": "node ./build/build.js && node ./build/pack.js",
    "build:clean": "cross-env BUILD_TARGET=clean node ./build/build.js",
    "build:web": "cross-env BUILD_TARGET=web node ./build/build.js",
    "build:native": "electron-rebuild && electron-rebuild -a ia32 && ls -R node_modules/sqlite3/lib/binding",
    "asar:list": "asar list ./dist/release/gxgd-check-win32-ia32/resources/app.asar",
    "dev": "node ./build/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config ./build/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config ./build/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix && npm run build:native"
  },
  "buildOptions": {
    "dir": ".",
    "ignore": [
      "/\\.",
      "/yarn",
      "/test"
    ],
    "out": "dist/release",
    "icon": "static/logo",
    "asar": true,
    "overwrite": true,
    "arch": [
      "ia32",
      "x64"
    ],
    "platform": "win32"
  },
  "dependencies": {
    "glob": "^7.1.2",
    "json-stringify-pretty-compact": "^1.0.4",
    "knex": "^0.13.0",
    "sqlite3": "^3.1.9",
    "vue": "^2.4.2",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.0",
    "walk": "^2.3.9",
    "xlsx-populate": "^1.10.0"
  },
  "devDependencies": {
    "archiver": "^2.0.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "1.7.5",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^9.0.1",
    "electron-rebuild": "^1.6.0",
    "element-ui": "^1.4.3",
    "eslint": "4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.1",
    "json-loader": "^0.5.7",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "markdown-loader": "^2.0.1",
    "mocha": "^3.5.0",
    "multispinner": "^0.2.1",
    "pdfjs-dist": "^1.9.564",
    "require-dir": "^0.3.2",
    "showdown": "^1.7.3",
    "spectron": "^3.7.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "12.2.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
